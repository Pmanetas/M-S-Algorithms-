{
  "productName": "MS Portal",
  "version": "0.1.0",
  "identifier": "com.manetas-stevens.portal",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "title": "Manetas & Stevens Portal",
        "center": true,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
} 